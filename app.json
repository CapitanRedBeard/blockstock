{
  "expo": {
    "name": "Blockstock",
    "description": "A cryptocurrency portfolio tracker ",
    "slug": "blockstock",
    "privacy": "public",
    "sdkVersion": "21.0.0",
    "version": "1.0.1",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/app-icon.png",
    "notification": {
      "icon": "./assets/icons/notification-icon.png",
      "color": "#000000"
    },
    "loading": {
      "icon": "./assets/icons/loading-icon.png"
    },
    "splash": {
      "image": "./assets/icons/splash.png",
      "backgroundColor": "#253B5B"
    },
    "packagerOpts": {
      "assetExts": ["ttf"]
    },
    "ios": {
      "bundleIdentifier": "com.capitanredbeard.blockstock",
      "supportsTablet": true
    },
    "android": {
      "package": "com.capitanredbeard.blockstock"
    }
  }
}
